{
  "name": "object-partners-presentation",
  "version": "1.0.0",
  "main": "./dist/webpack-config.js",
  "repository": {},
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "del-cli \"dist/**/*\"",
    "build": "npm run transpile & npm run style",
    "presass": "mkdirp dist/style",
    "sass": "node-sass src/style/slide-deck.scss > dist/style/slide-deck.css --include-path ./node_modules/reveal.js/css/theme/template",
    "prestyle": "npm run sass",
    "style": "postcss dist/style/slide-deck.css --output dist/style/slide-deck.css",
    "transpile": "babel src --out-dir dist",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "autoprefixer": "~6.7.6",
    "babel-cli": "~6.23.0",
    "babel-jest": "~19.0.0",
    "babel-plugin-add-module-exports": "~0.2.1",
    "babel-preset-env": "~1.1.11",
    "cssnano": "~3.10.0",
    "del-cli": "~0.2.1",
    "jest": "~19.0.2",
    "mkdirp": "~0.5.1",
    "node-sass": "~4.5.0",
    "postcss-assets": "~4.1.0",
    "postcss-cli": "~3.0.0-beta",
    "reveal.js": "~3.4.1"
  },
  "dependencies": {
    "angular2-template-loader": "~0.6.2",
    "awesome-typescript-loader": "~3.0.8",
    "css-loader": "~0.26.2",
    "deep-assign": "~2.0.0",
    "file-loader": "~0.10.1",
    "html-loader": "~0.4.5",
    "html-webpack-plugin": "~2.28.0",
    "lodash.flatten": "~4.4.0",
    "markdown-loader": "~2.0.0",
    "pug": "~2.0.0-beta11",
    "pug-html-loader": "~1.1.1",
    "raw-loader": "~0.5.1",
    "style-loader": "~0.13.2",
    "to-string-loader": "~1.1.5",
    "typescript": "~2.2.1",
    "url-loader": "~0.5.8",
    "webpack": "~2.2.1"
  }
}
