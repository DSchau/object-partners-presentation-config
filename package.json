{
  "name": "@objectpartners/presentation-config",
  "version": "1.0.0-rc.1",
  "main": "webpack-config.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/objectpartners/object-partners-presentation-config"
  },
  "license": "MIT",
  "author": "Dustin Schau <dustin.schau@objectpartners.com> (https://objectpartners.com)",
  "scripts": {
    "precommit": "lint-staged",
    "prebuild": "del-cli \"dist/**/*\"",
    "build": "babel src --out-dir dist --copy-files",
    "prebuild:publish": "npm run build && npm run copy",
    "build:publish": "cd dist && npm publish",
    "postbuild:publish": "cd ../",
    "copy": "cp package.json dist/package.json",
    "format": "prettier \"src/**/*.js\" --write --single-quote",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "babel-cli": "~6.23.0",
    "babel-jest": "~19.0.0",
    "babel-plugin-add-module-exports": "~0.2.1",
    "babel-preset-env": "^1.6.0",
    "del-cli": "~0.2.1",
    "husky": "^0.14.3",
    "jest": "~19.0.2",
    "lint-staged": "^4.0.4",
    "mkdirp": "~0.5.1",
    "prettier": "^1.6.1"
  },
  "dependencies": {
    "angular2-template-loader": "~0.6.2",
    "autoprefixer": "~6.7.6",
    "awesome-typescript-loader": "~3.0.8",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "~0.26.2",
    "cssnano": "~3.10.0",
    "deep-assign": "~2.0.0",
    "extract-text-webpack-plugin": "~2.0.0",
    "file-loader": "~0.10.1",
    "highlight.js": "~9.10.0",
    "html-loader": "~0.4.5",
    "html-webpack-plugin": "~2.28.0",
    "lodash.flatten": "~4.4.0",
    "markdown-loader": "~2.0.0",
    "node-sass": "~4.5.0",
    "postcss-loader": "~1.3.3",
    "pug": "~2.0.0-beta11",
    "pug-html-loader": "^1.1.5",
    "raw-loader": "~0.5.1",
    "sass-loader": "~6.0.2",
    "style-loader": "~0.13.2",
    "to-string-loader": "~1.1.5",
    "typescript": "~2.2.1",
    "url-loader": "~0.5.8",
    "webpack": "~2.2.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run format",
      "git add"
    ]
  }
}
